    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Orchestrator",
            "preLaunchTask": "${defaultBuildTask}",
            "program": "d:/src/botframework-cli/packages/orchestrator/bin/run",
            "args": [
                "orchestrator:create",
                "--in",
                "D:/src/Orchestrator/OrchestratorCli/Data/calendar/CreateMeeting",
            ],
            "internalConsoleOptions": "openOnSessionStart",
        }
    ]
}