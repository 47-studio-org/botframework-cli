    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Orchestrator",
            "preLaunchTask": "${defaultBuildTask}",
            "program": "d:/src/botframework-cli/packages/orchestrator/bin/run",
            "args": [
                "orchestrator:add",
                "-m",
                "D:/src/TScience/Orchestrator/oc/dep/model",
                "-i",
                "D:/src/Orchestrator/OrchestratorCli/Data/LU/skills/emailskill/en/email.lu",
                "-s",
                "D:/src/Orchestrator/Samples/DispatchBot/CognitiveModels",
                "-o",
                "D:/src/Orchestrator/Samples/DispatchBot/CognitiveModels",
                "--hierarchical"
            ],
            "internalConsoleOptions": "openOnSessionStart",
        }
    ]
}